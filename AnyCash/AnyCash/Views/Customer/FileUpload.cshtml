@model CORE.Tables.CASH_CUSTOMERS
@{
    ViewBag.Title = "Chứng từ đính kèm khách hàng";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .dz-preview.dz-processing.dz-image-preview.dz-success.dz-complete {
        text-align: center;
        right: 300px;
        margin-top: 70px;
    }
</style>
<div class="page-header page-header-default">
    <div class="page-header-content" style="background-color: #fcfcfc;">
        <div class="page-title" style="padding: 20px 0;">
            <h4>
                <a href="javascript:void(0);" onclick="history.go(-1);"><i class="icon-arrow-left52 position-left"></i></a>
                <span class="text-semibold">Chứng từ đính kèm của khách hàng</span>
            </h4>
        </div>
    </div>
</div>
<div class="div-content-fileupload">
    <div class="content-fileupload" style="display: flex; margin: 20px 25px; padding: 10px; background-color: #ffffff">
        <div class="porlet-content-file-upload" style="width: 25%; text-align: right;  padding-right: 40px;  padding-top: 10px; font-size: 15px;">
            <span class="text-black valign-text-top">Upload Ảnh khách hàng</span>
        </div>
        <div class="plupload_file_status" style="width: 75%">
            <div class="m-porlet-content-file-upload">
                <form action="/file-upload"
                      class="dropzone"
                      id="my-awesome-dropzone" onclick="uploadFile()"></form>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/customer/dropzone.js"></script>